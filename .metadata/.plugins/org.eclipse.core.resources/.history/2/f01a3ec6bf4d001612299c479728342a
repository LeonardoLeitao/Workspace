package net.anothergame.MODEL;

public class Jogador {
	private final int id;
	private String nome;
	private int score;
	private int socreMax;
	
	public Jogador(int id, String nome) {
		this.id = id;
		this.nome = nome;
		this.score = 0;
		this.socreMax = 0;
	}
	
	public int getID() {
		return this.id;
	}
	
	
}
